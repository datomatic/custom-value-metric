{
    "name": "datomatic/custom-value-metric",
    "description": "A Laravel Nova card.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "laravel/nova": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Datomatic\\CustomValueMetric\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Datomatic\\CustomValueMetric\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
